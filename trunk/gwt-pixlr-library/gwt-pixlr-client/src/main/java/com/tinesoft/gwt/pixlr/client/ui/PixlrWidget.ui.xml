<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:style>
		.container
		{
			width:100%;
			height:100%;
			position:relative;
		}
	</ui:style>
	<ui:with field="resources" type="com.tinesoft.gwt.pixlr.client.resources.PixlrWidgetResources"/>
	<g:FlowPanel styleName="{style.container}">
		<g:FormPanel ui:field="formPanel" styleName="{resources.css.formPanel}"/>
		<g:NamedFrame ui:field="targetFrame" styleName="{resources.css.targetFrame}"/>		
	</g:FlowPanel>
</ui:UiBinder> 